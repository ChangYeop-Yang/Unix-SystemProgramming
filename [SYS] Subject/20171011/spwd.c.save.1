/* Define Function  */
void printpathto(ino_t this_inode) {
	ino_t	my_inode;
	char	its_name[BUFSIZ];

	if (get_inode("..") != this_inode) {
		chdir("..");	/* Up One Directory  */
		inum_to_name(this_inode, its_name, BUFSIZ); 	/* Get Its Name  */

		my_inode = get_inode(".");
		printpathto(my_inode);
		printf("\%s", its_name);
	}
}
